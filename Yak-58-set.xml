<?xml version="1.0"?>

<PropertyList>
  
  <controls>
	  <engines>
	    <engine n="0">
	      <cowl-flaps-norm>0</cowl-flaps-norm>
	      <master-bat>false</master-bat>
	      <master-alt>false</master-alt>
	      <primer type="bool">false</primer>
	      <fuel-pump type="bool">false</fuel-pump>
	    </engine>
	    
	    </engines>
	    
	    <electric>
	      <external-power type ="bool">false</external-power>
	      <battery-switch type ="bool">false</battery-switch>
	    </electric>
	    
	    <gear>
	      <brake-parking>1</brake-parking>
	    </gear>
	    
	    <flight>
	      <aileron-trim type="double">0.0</aileron-trim>
	      <elevator-trim type="double">0.0</elevator-trim>
	      <rudder-trim type="double">0.0</rudder-trim> 
	    </flight>
	    
	    <lighting>
	      <beacon type="bool">false</beacon>
	      <strobe type="bool">false</strobe>
	      <nav-lights type="bool">false</nav-lights>
	      <landing-lights type="int">0</landing-lights>
              <taxi-lights type="int">0</taxi-lights>
	      <panel-norm type="double">0.0</panel-norm>
	    </lighting>
	  </controls>
	  
	  <scenery><share-events type="bool">true</share-events></scenery>
  
  <sim>
    <author>Herbert Wagner 2015</author>
    <description>Yak-58</description>
    <aircraft-version>2.0.2</aircraft-version>
    <rating>
      <FDM type="int">5</FDM>
      <systems type="int">5</systems>
      <cockpit type="int">5</cockpit>
      <model type="int">5</model>
    </rating>
    <status>production</status>
    <flight-model>jsb</flight-model>
    <aero>Yak-58-JSBSim-2</aero>
    
    <!-- Configuracion de la vista del piloto -->
    
    <view>
      <internal archive="y">true</internal>
      <config>
	<x-offset-m archive="y">-0.33</x-offset-m>  <!-- Lateral -->
	<y-offset-m archive="y"> 0.53</y-offset-m>  <!-- Altura -->
	<z-offset-m archive="y">-2.05</z-offset-m>  <!-- Profundidad -->
	<pitch-offset-deg>-8</pitch-offset-deg>
	<default-field-of-view-deg type="double">85.0</default-field-of-view-deg>
	<limits>
	  <enabled type="bool">true</enabled>
	  <left>
	    <heading-max-deg type="double">165</heading-max-deg>
	    <x-offset-max-m type="double">0.15</x-offset-max-m>
	    <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
	  </left>
	  <right>
	    <heading-max-deg type="double">165</heading-max-deg> 
	    <x-offset-max-m type="double">0.15</x-offset-max-m>
	    <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
	  </right>
	</limits>
      </config>
    </view>
    
    <view n="100">
      <name>Copilot view</name>
      <type>lookfrom</type>
      <internal type="bool">true</internal>
      <config>
	<from-model type="bool">true</from-model>
	<x-offset-m archive="y"> 0.33</x-offset-m>  <!-- Lateral -->
	<y-offset-m archive="y"> 0.53</y-offset-m>  <!-- Altura -->
	<z-offset-m archive="y">-2.05</z-offset-m>  <!-- Profundidad -->
	<pitch-offset-deg>-8</pitch-offset-deg>
	<default-field-of-view-deg type="double">85.0</default-field-of-view-deg>
	<limits>
	  <enabled type="bool">true</enabled>
	  <left>
	    <heading-max-deg type="double">265</heading-max-deg>
	    <x-offset-max-m type="double">0.15</x-offset-max-m>
	    <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
	  </left>
	  <right>
	    <heading-max-deg type="double">265</heading-max-deg> 
	    <x-offset-max-m type="double">0.15</x-offset-max-m>
	    <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
	  </right>
	</limits>
      </config>
    </view>
    
    <view n="101">
      <name>Passenger view</name>
      <type>lookfrom</type>
      <internal type="bool">true</internal>
      <config>
	<from-model type="bool">true</from-model>
	<x-offset-m archive="y"> 0.270</x-offset-m>  <!-- Lateral -->
	<y-offset-m archive="y"> 0.73</y-offset-m>  <!-- Altura -->
	<z-offset-m archive="y"> 0.0</z-offset-m>  <!-- Profundidad -->
	<pitch-offset-deg>-8</pitch-offset-deg>
	<default-field-of-view-deg type="double">85.0</default-field-of-view-deg>
	<limits>
	  <enabled type="bool">true</enabled>
	  <left>
	    <heading-max-deg type="double">265</heading-max-deg>
	    <x-offset-max-m type="double">0.15</x-offset-max-m>
	    <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
	  </left>
	  <right>
	    <heading-max-deg type="double">265</heading-max-deg> 
	    <x-offset-max-m type="double">0.15</x-offset-max-m>
	    <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
	  </right>
	</limits>
      </config>
    </view>
    
    <view n="102">
      <name>UTILITY VIEW</name>
      <type>lookfrom</type>
      <internal type="bool">true</internal>
      <config>
	<from-model type="bool">true</from-model>
	<x-offset-m archive="y"> 0    </x-offset-m>  <!-- Lateral -->
	<y-offset-m archive="y"> 0.53</y-offset-m>  <!-- Altura -->
	<z-offset-m archive="y">-2.10</z-offset-m>  <!-- Profundidad -->
	<pitch-offset-deg>-28</pitch-offset-deg>
	<default-field-of-view-deg type="double">85.0</default-field-of-view-deg>
	<limits>
	  <enabled type="bool">false</enabled>
	  <left>
	    <heading-max-deg type="double">165</heading-max-deg>
	    <x-offset-max-m type="double">0.15</x-offset-max-m>
	    <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
	  </left>
	  <right>
	    <heading-max-deg type="double">165</heading-max-deg> 
	    <x-offset-max-m type="double">0.15</x-offset-max-m>
	    <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
	  </right>
	</limits>
      </config>
    </view>
    
    <startup>
      <splash-texture>Aircraft/Yak-58/Yak-58-splash.png</splash-texture>
    </startup>
    
    <gui>
      <style>
	<colors>
	  <splash-screen>
	    <red>  1</red>
	    <green>1</green>
	    <blue> 1</blue>
	  </splash-screen>
	</colors>
      </style>
    </gui>
    
    <sound>
      <path>Aircraft/Yak-58/Yak-58-sound.xml</path>
    </sound>
    
    <model>
      <path archive="y">Aircraft/Yak-58/Models/Yak-58.xml</path>
      <livery>
	<file type="string">Default</file>
      </livery>
      <Yak-58><version type="string">2.0.1</version></Yak-58>
    </model>
    
    <crashed type="bool">0</crashed>
    <yokes-visible type="bool">1</yokes-visible>
    
    <menubar include="Dialogs/Yak-58-menu.xml"/>
    <help include="Dialogs/Yak-58-help.xml"/>
    
    <systems>
      <path>Aircraft/Yak-58/Systems/Yak-58-systems.xml</path>
      <autopilot>
	<path>Aircraft/Yak-58/Systems/Yak-58-autopilot.xml</path>
      </autopilot>
      <property-rule n="100">
	<name>Instrument-Filter</name>
	<path>Aircraft/Yak-58/Systems/Yak-58-filters.xml</path>
      </property-rule>
    </systems>
    
    <instrumentation>
      <path>Aircraft/Yak-58/Systems/Yak-58-instrumentation.xml</path>
    </instrumentation>
    
    <multiplay>
      <chat_display>1</chat_display>
      
      <generic>
	
	<int type="int" n="1" alias="/controls/lighting/beacon-state/state"/>
	
	<int type="int" n="8" alias="/controls/lighting/nav-lights"/>
	<int type="int" n="9" alias="/controls/lighting/landing-lights"/>
        <int type="int" n="10" alias="/controls/lighting/taxi-lights"/>
	<int type="int" n="17" alias="/sim/signals/fdm-initialized" />	
	
	<float n="0" alias="/sim/model/door-positions/fr/position-norm"/>
	<float n="1" alias="/sim/model/door-positions/fl/position-norm"/>
	<float n="2" alias="/position/altitude-agl-ft"> 0 </float>
	<float n="3" alias="/sim/model/door-positions/br/position-norm"/>
	<float n="4" alias="/fdm/jsbsim/fcs/steer-pos-deg"/>
	<float n="5" alias="/controls/lighting/panel-norm"/>
	<float n="9" alias="/engines/engine/thruster/pitch"/>
	<float n="10" alias="/gear/gear[0]/rollspeed-ms" />
	<float n="11" alias="/gear/gear[1]/rollspeed-ms" />
	<float n="12" alias="/gear/gear[2]/rollspeed-ms" />
	<float n="13" alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[1]" />
	<float n="14" alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[2]" />
	<float n="15" alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[3]" />
	<float n="16" alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[4]" />
	
	<string  alias="/sim/multiplay/callsign"/> 
	<string n="1" type="string" alias="/sim/model/Yak-58/version"/>
	
	</generic>
	
	</multiplay>
	
	<import>
      <path>Huds/Sets/controls.xml</path>
      <x-offset>-100</x-offset>
      <y-offset>70</y-offset>
    </import> 
	
	</sim>
	
	<autopilot>
	  <locks>
	    <altitude type="string"></altitude>
	    <heading type="string"></heading>
	  </locks>
	  <settings>
	    <bank-auto type="double">20.0</bank-auto>
	    <bank-select type="double">0</bank-select>
	    <pitch-select type="double">0</pitch-select>
	  </settings>
	</autopilot>
	
	<gear>
	  <gear n="0">
	    <tyre-smoke type="int">0</tyre-smoke>
	    <sprayspeed-ms type="double">0</sprayspeed-ms>
