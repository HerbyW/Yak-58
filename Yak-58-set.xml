	  <nasal>
	    <doors>
	      <file>Aircraft/Yak-58/Nasal/doors.nas</file>
	    </doors>
	    <Yak-58>
	      <file>Aircraft/AN-12BK/Nasal/autopilot.nas</file>
	      <file>Aircraft/Yak-58/Nasal/light.nas</file>
	      <file>Aircraft/Yak-58/Nasal/liveries.nas</file>
