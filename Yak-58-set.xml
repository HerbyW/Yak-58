	    <terrain>
	    <file>Aircraft/Yak-58/Nasal/terrain.nas</file>
	  </terrain>
	  </nasal>
	  
	  <instrumentation>
	    <adf n="0">
	      <power-btn type="bool">true</power-btn>
	      <volume-norm type="double">0</volume-norm>
	      <adf-btn type="bool">true</adf-btn>
	      <mode type="string">adf</mode>
	    </adf>
	    <dme n="0">
	      <power-btn type="bool">true</power-btn>
	      <mode-btn type="int">0</mode-btn>
	    </dme>
