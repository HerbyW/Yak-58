	    </instrumentation>
	    
	    
	    
	    <input>
	      <keyboard>
		
		<key n="100">
		  <name>d</name>
		  <desc>Open/Close door right</desc>
		  <binding>
		    <command>nasal</command>
		    <script>doors.fr.toggle();</script>
		  </binding>
		  <binding>
		    <command>nasal</command>
		    <script>doors.br.toggle();</script>
		  </binding>
		</key>
		
		<key n="68">
		  <name>D</name>
		  <desc>Open/Close door left</desc>
		  <binding>
		    <command>nasal</command>
		    <script>doors.fl.toggle();</script>
		  </binding>
		</key>
		
		<key n="83">
		  <name>S</name>
		  <desc>Autostart</desc>
		  <binding>
		    <command>property-toggle</command>
		    <property>sim/model/autostart</property>
		    <value>true</value>
		  </binding>
		</key>
		
		<key n="60">
		  <name> > </name>
		<desc>Change Pilot Engineer View</desc>
		<binding>
		  <command>property-cycle</command>
		  <property>/sim/current-view/view-number</property>  
		  <value>0</value>
		  <value>9</value>
		</binding>
	      </key>	
	    </keyboard>
	  </input>
	  
